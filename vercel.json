{
    "buildCommand": "cd project-management-frontend && npm install && npm run build",
    "outputDirectory": "project-management-frontend/build",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}